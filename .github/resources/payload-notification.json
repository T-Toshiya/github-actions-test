{
  "attachments": [
    {
      "color": "#0953FF",
      "blocks": [
        {
          "type": "header",
          "text": {
            "type": "plain_text",
            "text": "Start manually deploy to test."
          }
        },
        {
          "type": "section",
          "fields": [
            {
              "type": "mrkdwn",
              "text": "*repo:*\n<{{ github.payload.repository.html_url }}|{{ github.payload.repository.name }}>"
            },
            {
              "type": "mrkdwn",
              "text": "*commit:*\n< {{ github.event.head_commit.url }}| {{ github.event.head_commit.message }}>"
            },
            {
              "type": "mrkdwn",
              "text": "*ref:*\n {{ github.ref }}"
            },
            {
              "type": "mrkdwn",
              "text": "*workflow:*\n< {{ github.payload.event }}/actions/runs/${{ github.runId }}|{{ github.workflow }}>"
            }
          ]
        },
        {
          "type": "context",
          "elements": [
            {
              "type": "mrkdwn",
              "text": "*Actor*: {{ github.actor }}"
            },
            {
              "type": "image",
              "image_url": "{{ github.payload.sender.avatar_url }}",
              "alt_text": "actor"
            }
          ]
        },
        {
          "type": "divider"
        }
      ]
    }
  ]
}